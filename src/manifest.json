{
  "name": "md-new-tab-vue-2",
  "description": "A Vue.js web extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "md-new-tab-vue-2",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "chrome_url_overrides" : {
    "newtab": "new-tab/new-tab.html"
  },
  "permissions": [
    "http://suggestqueries.google.com/*"
  ]
}
